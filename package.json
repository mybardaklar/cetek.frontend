{
	"name": "cetek.frontend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"prettier": "prettier . --write",
		"dev:tailwindcss": "tailwindcss -i ./src/assets/css/tailwind.css -o ./public/css/tailwind.css --watch",
		"dev:server": "env-cmd -f .env nodemon src/index.ts",
		"dev": "concurrently \"pnpm:dev:tailwindcss\" \"pnpm:dev:server\"",
		"build:tailwindcss": "tailwindcss build -i ./src/assets/css/tailwind.css -o ./public/css/tailwind.css",
		"build:server": "tsc -p .",
		"build:local": "npm run build:tailwindcss && npm run build:server",
		"local": "npm run build:local && env-cmd -f .env node src/index.ts",
		"asda": "npm run build:tailwindcss && ts-node src/index.ts"
	},
	"dependencies": {
		"@netlify/functions": "^2.8.1",
		"axios": "^1.7.2",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"dotenv": "^16.4.5",
		"env-cmd": "^10.1.0",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"i18next": "^23.12.1",
		"i18next-fs-backend": "^2.3.1",
		"i18next-http-middleware": "^3.6.0",
		"morgan": "~1.9.1",
		"pug": "2.0.0-beta11",
		"serverless-http": "^3.2.0"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.13",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/http-errors": "^2.0.4",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.0.0",
		"autoprefixer": "^10.4.19",
		"concurrently": "^8.2.2",
		"nodemon": "^3.1.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.39",
		"postcss-cli": "^11.0.0",
		"postcss-sort-media-queries": "^5.2.0",
		"prettier": "^3.3.2",
		"tailwindcss": "^3.4.6",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4"
	}
}
